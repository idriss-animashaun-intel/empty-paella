<?xml version="1.0" encoding="utf-8"?>
<ifpm_setup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="GEN_IFPM_tt.xsd">

  <regex_list>
    <regex name="scn_regex">
      <value>*R4004606A*_CORE_sEs_post_seq</value>
    </regex>
  </regex_list>

  <pattern_modify_group_definitions>
    <pattern_modify_group name="CORE" > <!--  ModifyGroup taken from  set_point_group_base  parameter -->
      <subset name="CORE7"> <!--  This is Core0 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>43</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
      <subset name="CORE6"> <!--  This is Core1 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>44</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
	  <subset name="CORE5"> <!--  This is Core2 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>45</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
      <subset name="CORE4"> <!--  This is Core3 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>46</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
	  <subset name="CORE3"> <!--  This is Core4 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>47</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
      <subset name="CORE2"> <!--  This is Core5 -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="label">CHECK_SCRBD_FOR_CORES_STATUS_DR1</start_position>
        <offset>48</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
	  <subset name="CORE1"> <!--  This is Atom LLC but is not used for Core Testing -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="address">3</start_position>
        <offset>0</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
	  <subset name="CORE0"> <!--  This is Atom LLC but is not used for Core Testing -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="address">3</start_position>
        <offset>0</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
    </pattern_modify_group>
    <pattern_modify_group name="DUMMY">
	<subset name="DUMMY"> <!--  CORE7 is for ATOM LLC and not used for CORE testing -->
        <pattern_regex_name type="main">scn_regex</pattern_regex_name>
        <domain>LEG</domain>
        <start_position type="address">3</start_position>
        <offset>0</offset>
        <data_type>PIN</data_type>
        <pin_name>TDO</pin_name>
      </subset>
    </pattern_modify_group>
  </pattern_modify_group_definitions>

  <pattern_modify_group_data name="CORE" allow_different_setpoint_names="true" >
    <subset name="CORE7">
      <setpointdata name="disableSetPoin7">X</setpointdata> <!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint7">L</setpointdata> <!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
    <subset name="CORE6">
      <setpointdata name="disableSetPoin6">X</setpointdata>	<!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint6">L</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
    <subset name="CORE5">
      <setpointdata name="disableSetPoin5">X</setpointdata>	<!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint5">L</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
    <subset name="CORE4">
      <setpointdata name="disableSetPoin4">X</setpointdata>	<!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint4">L</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
	<subset name="CORE3">
      <setpointdata name="disableSetPoin3">X</setpointdata>	<!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint3">L</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
	<subset name="CORE2">
      <setpointdata name="disableSetPoin2">X</setpointdata>	<!--  disableSetPoint taken from disable_ifpm_setpoint dierecovery parameter and applied the User Define Patmod Value to a Vector address -->
	  <setpointdata name="enableSetPoint2">L</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
	<subset name="CORE1">
      <setpointdata name="disableSetPoin1">X</setpointdata>	<!-- Not Used for Core Testing but needs to be defined as this file fails during TP execution if definition for each bit in Slice Tracker is not Defined here -->
	  <setpointdata name="enableSetPoint1">X</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
	<subset name="CORE0">
      <setpointdata name="disableSetPoin0">X</setpointdata>	<!-- Not Used for Core Testing but needs to be defined as this file fails during TP execution if definition for each bit in Slice Tracker is not Defined here -->
	  <setpointdata name="enableSetPoint0">X</setpointdata>	<!--  enableSetPoint taken from enable_ifpm_setpoint dierecovery parameter and this Resets Pattern back to Original State -->
    </subset>
  </pattern_modify_group_data>
  <pattern_modify_group_data name="DUMMY" allow_different_setpoint_names="true" >	<!-- Dummy Sequence needed to be added to IFPM Modify Group else TOS3.9 complains -->
  	<subset name="DUMMY">
      <setpointdata name="NONE">X</setpointdata>
    </subset>
  </pattern_modify_group_data>

</ifpm_setup>

